{
  "name": "ngts-project-template",
  "version": "0.0.1",
  "scripts": {
    "build": "rimraf dist && webpack -p --config conf/webpack.prod.js",
    "build:dev": "rimraf dist && webpack --config conf/webpack.base.js",
    "serve": "webpack-dev-server --config conf/webpack.base.js",
    "serve:prod": "rimraf dist && webpack-dev-server --config conf/webpack.prod.js",
    "test": "rimraf reports && node ./scripts/test html",
    "test:ci": "rimraf reports && node ./scripts/test ci",
    "test:auto": "karma start --no-single-run conf/karma.conf.js"
  },
